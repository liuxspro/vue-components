{
  "name": "@liuxspro/vue-components",
  "version": "0.1.0",
  "description": "my vue-components",
  "keywords": [
    "vue3",
    "components"
  ],
  "license": "MIT",
  "author": "liuxspro<liuxspro@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/liuxspro/vue-components.git"
  },
  "homepage": "https://github.com/liuxspro/vue-components#readme",
  "bugs": {
    "url": "https://github.com/liuxspro/vue-components/issues"
  },
  "type": "module",
  "private": false,
  "main": "./dist/lib.js",
  "module": "./dist/lib.js",
  "types": "./dist/lib.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "build:lib": "vue-tsc -b --declaration && vite build --mode lib",
    "type-check": "vue-tsc -b",
    "clean": "rimraf dist",
    "release": "npm run clean && npm run build:lib"
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/language-core": "^3.2.4",
    "@vue/tsconfig": "^0.8.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.1.4"
  },
  "peerDependencies": {
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
